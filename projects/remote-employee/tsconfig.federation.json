{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "shared-ui": [
        "../../dist/shared-ui"
      ]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
